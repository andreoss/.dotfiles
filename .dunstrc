[global]
alignment="center"
font="Terminus"
frame_color="#999999"
height=96
ignore_newline=no
line_height=2
offset=32
origin="top-right"
separator_color="#D3D3CE"
width=256
word_warp="true"

[urgency_critical]
background="#FCF3E4"
foreground="#1E1E1E"
frame_color="#C9002B"
timeout=0

[urgency_low]
background="#D3D3CE"
foreground="#001001"
timeout=3600

[urgency_normal]
background="#D3D3CE"
foreground="#001001"
timeout=180
